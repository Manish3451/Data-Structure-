#include <iostream>
using namespace std;

int main() {
	// your code goes here
    int marks[100];
    int n;
    cout<<"Enter the No of students"<<endl;
    cin>>n;

    //input
    for(int i=0; i <= n; i++){
        cin>>marks[i];
        marks[i] = marks[i] * 3
    }
    //output
    for(int i=0; i <= n; i++){
        cout<<marks[i]<<endl;
    }

    
	return 0;
}
